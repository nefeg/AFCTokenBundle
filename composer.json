{
  "name": "path-to-repo/afc-tokens",
  "description": "Token service",
  "keywords": ["token", "security"],
  "type": "symfony-bundle",
  "license": "MIT",
  "version": "1.1.2-RC1",

  "repositories": [
    {
      "type": "vcs",
      "url": "set path to repo git here"
    }
  ],

  "require": {
    "php": "^7.1.3",
    "firebase/php-jwt": "^5.0",
    "jms/serializer-bundle": "^3.4",
    "ramsey/uuid": "^3.7",
    "symfony/console": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/lts": "^4@dev",
    "symfony/monolog-bundle": "^3.2",
    "symfony/orm-pack": "^1.0",
    "symfony/proxy-manager-bridge": "^4.0",
    "symfony/yaml": "^4.0",
    "path-to-repo/libjc": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Aimchat\\AFCTokenBundle\\": "src/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
